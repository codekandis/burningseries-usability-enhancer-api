{
	"name": "codekandis/burningseries-usability-enhancer-api",
	"description": "This is an API for `burningseries.co` usability enhancements.",
	"version": "0.8.0",
	"keywords": [
		"php",
		"burningseries",
		"usability",
		"enhancer",
		"api"
	],
	"minimum-stability": "stable",
	"prefer-stable": true,
	"license": "MIT",
	"authors": [
		{
			"name": "Christian Ramelow",
			"email": "info@codekandis.net",
			"homepage": "https://codekandis.net",
			"role": "Developer"
		}
	],
	"support": {
		"email": "info@codekandis.net",
		"issues": "https://github.com/codekandis/burningseries-usability-enhancer-api/issues",
		"docs": "https://github.com/codekandis/burningseries-usability-enhancer-api/blob/master/README.md",
		"source": "https://github.com/codekandis/burningseries-usability-enhancer-api/tree/master"
	},
	"require": {
		"php": "^7.4",
		"codekandis/tiphy": "^0",
		"codekandis/sentry-client": "^1",
		"codekandis/tiphy-sentry-client-integration": "^0",
		"codekandis/authentication": "^0"
	},
	"require-dev": {
		"roave/security-advisories": "dev-master",
		"codekandis/phpunit": "^3"
	},
	"repositories": {
	},
	"autoload": {
		"psr-4": {
			"CodeKandis\\BurningSeriesUsabilityEnhancerApi\\": [
				"config/",
				"public/",
				"src/"
			]
		}
	},
	"autoload-dev": {
		"psr-4": {
			"CodeKandis\\BurningSeriesUsabilityEnhancerApi\\Tests\\": [
				"tests/"
			]
		}
	},
	"config": {
		"allow-plugins": {
			"php-http/discovery": false
		}
	}
}
